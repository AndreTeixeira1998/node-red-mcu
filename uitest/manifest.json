{
	"include": [
		"$(MODDABLE)/examples/manifest_base.json",
		"$(MODDABLE)/examples/manifest_piu.json",
		"$(MODDABLE)/modules/base/deepEqual/manifest.json",
		"$(MODDABLE)/modules/base/structuredClone/manifest.json",
		"$(MODDABLE)/modules/base/modules/manifest.json",
		"$(MODDABLE)/modules/data/base64/manifest.json",
		"$(MODDABLE)/modules/data/hex/manifest.json",
		"$(MODDABLE)/modules/piu/MC/outline/manifest.json"
	],
	"creation": {
		"keys": {
			"available": 128
		},
		"stack": 1024
	},
	"modules": {
		"*": [
			"../nodered",
			"./ui_nodes",
			"./ScrollerBehaviors",
			"./ui_templates",
			"./main",
			{
				"source": "./flows",
				"transform": "nodered2mcu"
			}
		]
	},
	"preload": [
		"nodered",
		"ui_nodes",
		"ui_templates",
		"flows"
	],
	"resources":{
		"*-mask": [
			"$(MODDABLE)/examples/assets/fonts/OpenSans-Regular-18",
			"$(MODDABLE)/examples/assets/fonts/OpenSans-Semibold-18",
			"./assets/button",
			"./assets/glyphs",
			"./assets/popup"
		]
	}
}
