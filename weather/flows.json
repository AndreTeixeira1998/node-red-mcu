    {
        "id": "eea6aa4e9aedba85",
        "type": "function",
        "z": "43412a74034aa593",
        "name": "prepare",
        "func": "let input, output;\n\ninput = msg.payload.winddirection;\nif (input > 11.25 && input <= 33.75)\n    output = \"NNE\";\nelse if (input > 33.75 && input < 56.25)\n    output = \"NE\";\nelse if (input > 56.25 && input < 78.75)\n    output = \"ENE\";\nelse if (input > 78.75 && input < 101.25)\n    output = \"E\";\nelse if (input > 101.25 && input < 123.75)\n    output = \"ESE\";\nelse if (input > 123.75 && input < 146.25)\n    output = \"SE\";\nelse if (input > 146.25 && input < 168.75)\n    output = \"SSE\";\nelse if (input > 168.75 && input < 191.25)\n    output = \"S\";\nelse if (input > 191.25 && input < 213.75)\n    output = \"SSW\";\nelse if (input > 213.75 && input < 236.25)\n    output = \"SW\";\nelse if (input > 236.25 && input < 258.75)\n    output = \"WSW\";\nelse if (input > 258.75 && input < 281.25)\n    output = \"W\";\nelse if (input > 281.25 && input < 303.75)\n    output = \"WNW\";\nelse if (input > 303.75 && input < 326.25)\n    output = \"NW\";\nelse if (input > 326.25 && input < 348.75)\n    output = \"NNW\";\nelse\n    output = \"N\";\nmsg.payload.winddirection = output;\n\ninput = msg.payload.windspeed;\noutput = Math.round((input * 3600) / 1000)\nmsg.payload.windspeed = output;\n\ninput = msg.payload.icon;\nif (input == '01d') output = 'wi-wu-sunny'; // clear sky\nelse if (input == '02d') output = 'wi-wu-partlysunny'; // few clouds\nelse if (input == '03d') output = 'wi-wu-cloudy'; // scattered clouds\nelse if (input == '04d') output = 'wi-wu-cloudy'; // broken clouds\nelse if (input == '09d') output = 'wi-wu-sleet'; // shower rain\nelse if (input == '10d') output = 'wi-wu-rain'; // rain\nelse if (input == '11d') output = 'wi-wu-tstorms'; // thunderstorm\nelse if (input == '13d') output = 'wi-wu-snow'; // snow\nelse if (input == '50d') output = 'wi-wu-fog'; // mist\nelse if (input == '01n') output = 'wi-wu-nt_sunny'; // clear sky\nelse if (input == '02n') output = 'wi-wu-nt_partlysunny'; // few clouds\nelse if (input == '03n') output = 'wi-wu-nt_cloudy'; // scattered clouds\nelse if (input == '04n') output = 'wi-wu-nt_cloudy'; // broken clouds\nelse if (input == '09n') output = 'wi-wu-nt_sleet'; // shower rain\nelse if (input == '10n') output = 'wi-wu-nt_rain'; // rain\nelse if (input == '11n') output = 'wi-wu-nt_tstorms'; // thunderstorm\nelse if (input == '13n') output = 'wi-wu-nt_snow'; // snow\nelse if (input == '50n') output = 'wi-wu-nt_fog'; // mist\nmsg.payload.icon = output;\n\nreturn msg;",
